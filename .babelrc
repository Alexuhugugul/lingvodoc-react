{
  "plugins": [
    [
      "transform-runtime", {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ],
    "react-hot-loader/babel"
  ],
  "presets": [
    ["es2015", {"modules": false}],
    "stage-0",
    "react"
  ],
  "env": {
    "production": {
        "presets": ["react-optimize"]
    }
  }
}
